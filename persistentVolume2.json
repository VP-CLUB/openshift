{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "pv0002"
  },
  "spec": {
    "capacity": {
      "storage": "10Gi"
    },
    "accessModes": [ "ReadWriteOnce" ],
    "nfs": {
      "path": "/var/lib/nfs",
      "server": "172.16.5.58"
    },
    "persistentVolumeReclaimPolicy": "Recycle"
  }
}
